<?xml version='1.0'?>
<sdf version='1.9'>
  <model name='iris'> <pose>0 0 0.194923 0 0 0</pose>
    <link name='base_link'>
      <sensor name="imu_sensor" type="imu">
        <always_on>1</always_on>
        <update_rate>250</update_rate>
      </sensor>


      <inertial>
        <mass>1.5</mass> <inertia>
          <ixx>0.008</ixx><ixy>0</ixy><ixz>0</ixz>
          <iyy>0.015</iyy><iyz>0</iyz>
          <izz>0.017</izz> </inertia>
      </inertial>
      
      <collision name='base_collision'>
        <pose>0 0 -0.08 0 0 0</pose>
        <geometry>
          <box><size>0.47 0.47 0.23</size></box> </geometry>
      </collision>

      <visual name='base_visual'>
        <geometry>
          <mesh>
            <uri>model://iris/meshes/iris.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <sensor name="camera" type="camera">
        <pose>0.1 0 0 0 0 0</pose>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
        <camera>
          <horizontal_fov>1.047</horizontal_fov>
          <image><width>640</width><height>480</height></image>
          <clip><near>0.1</near><far>100</far></clip>
        </camera>
      </sensor>

      <visual name='front_left_leg_visual'>
        <pose>0.123 0.22 -0.11 0 0 0</pose>
        <geometry><cylinder><radius>0.005</radius><length>0.17</length></cylinder></geometry>
      </visual>
      </link>

    <plugin filename="libArduPilotPlugin.so" name="ArduPilotPlugin">
      <controller ip="127.0.0.1" port="9002"/>
      <imuName>imu_sensor</imuName>
      <control_type>velocity_control</control_type> 
      <rotor id="0">
        <jointName>rotor_0_joint</jointName>
      </rotor>
      <rotor id="1">
        <jointName>rotor_1_joint</jointName>
      </rotor>
      <rotor id="2">
        <jointName>rotor_2_joint</jointName>
      </rotor>
      <rotor id="3">
        <jointName>rotor_3_joint</jointName>
      </rotor>
    </plugin>

    <link name='rotor_0'>
      <pose>0.13 -0.22 0.023 0 0 0</pose>
      <visual name='rotor_0_visual'>
        <geometry><mesh><uri>model://iris/meshes/iris_prop_ccw.dae</uri></mesh></geometry>
      </visual>
    </link>
    <joint name='rotor_0_joint' type='revolute'>
      <child>rotor_0</child><parent>base_link</parent>
      <axis><xyz>0 0 1</xyz></axis>
    </joint>
    </model>
</sdf>